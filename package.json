{
  "name": "order_forder_front",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start": "electron dist/main.js",
    "dev": "concurrently \"npm run copy-files\" \"npm run watch:typescript\" \"npm run watch:webpack\" \"npm start\"",

    "create:dist": "concurrently  \"npm run copy-files\" \"npm run build:typescript\"",
    "create:build": "concurrently \"npm run create:dist\" \"npm run build:webpack\"",

    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.js dist/",
    "watch:webpack": "webpack --config webpack.common.js --watch",
    "watch:typescript": " tsc -p ./ -watch",
    "build:webpack": "webpack --config webpack.common.js",
    "build:typescript": " tsc -p ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.0",
    "@babel/preset-env": "^7.13.0",
    "@babel/preset-react": "^7.12.13",
    "@types/electron": "^1.6.10",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.0.2",
    "electron": "^11.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "mkdirp": "^1.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.17"
  }
}
